  "title": "NUnit .NET Core Runner Tests",
  "name": "dotnet-test-nunit.test",
  "version": "3.4.0-*",
  "buildOptions": {
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "NUnit": "3.4.0",
    "dotnet-test-nunit": {
      "target": "project"
    }
  },
  "testRunner": "nunit",

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dotnet5.6",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    },
    "net451": {}
  }
}
