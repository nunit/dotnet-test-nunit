{
  "copyright": "Copyright (C) 2016 Charlie Poole",
  "authors": [ "Rob Prouse" ],
  "title": "NUnit .NET Core Runner Test Runner",
  "name": "dotnet-test-nunit.test.runner",
  "version": "3.3.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "NUnitLite": "3.4.0",
    "dotnet-test-nunit": {
      "target": "project"
    },
    "dotnet-test-nunit.test": {
      "target": "project"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "net451": {}
  }
}
